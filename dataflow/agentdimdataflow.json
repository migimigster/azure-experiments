{
	"name": "agentdimdataflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "agent_source_dataset",
						"type": "DatasetReference"
					},
					"name": "Aagentsource1"
				},
				{
					"dataset": {
						"referenceName": "agent_source_dataset",
						"type": "DatasetReference"
					},
					"name": "Bagentsource2"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "leftjoin1",
					"description": "left join s_agent1 to s_agent2 via unit_manager_id = agent_id"
				},
				{
					"name": "agentmasterlistselectA"
				},
				{
					"name": "MANAGERFULLNAMECONCAT",
					"description": "concatenate agent names"
				},
				{
					"name": "agentmasterlistselectB",
					"description": "agent_id"
				},
				{
					"name": "sumcasedrcount"
				},
				{
					"name": "DRCOUNTGROUPBY"
				},
				{
					"name": "SOURCETODRCOUNT"
				},
				{
					"name": "FINALSELECT"
				},
				{
					"name": "selectagent"
				},
				{
					"name": "managerselect"
				},
				{
					"name": "concatname"
				}
			],
			"scriptLines": [
				"source(output(",
				"          AGENT_ID as string,",
				"          AGENT_FIRSTNAME as string,",
				"          AGENT_LASTNAME as string,",
				"          UNIT_MANAGER_ID as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     partitionBy('roundRobin', 2)) ~> Aagentsource1",
				"source(output(",
				"          AGENT_ID as string,",
				"          AGENT_FIRSTNAME as string,",
				"          AGENT_LASTNAME as string,",
				"          UNIT_MANAGER_ID as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     partitionBy('roundRobin', 2)) ~> Bagentsource2",
				"MANAGERFULLNAMECONCAT, agentmasterlistselectB join(AGENT_ID_A == UNIT_MANAGER_ID,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> leftjoin1",
				"Aagentsource1 select(mapColumn(",
				"          AGENT_ID_A = AGENT_ID,",
				"          AGENT_FIRSTNAME,",
				"          AGENT_LASTNAME",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> agentmasterlistselectA",
				"agentmasterlistselectA derive(AGENT_FULLNAME = concat(AGENT_FIRSTNAME,' ', AGENT_LASTNAME)) ~> MANAGERFULLNAMECONCAT",
				"Bagentsource2 select(mapColumn(",
				"          AGENT_ID_B = AGENT_ID,",
				"          UNIT_MANAGER_ID",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> agentmasterlistselectB",
				"leftjoin1 derive(DR_COUNT = iif(UNIT_MANAGER_ID != AGENT_ID_A, 1, 0)) ~> sumcasedrcount",
				"sumcasedrcount aggregate(groupBy(UNIT_MANAGER_ID,",
				"          AGENT_ID_B,",
				"          AGENT_FULLNAME),",
				"     DR_COUNT = sum(DR_COUNT)) ~> DRCOUNTGROUPBY",
				"selectagent, managerselect join(selectagent@UNIT_MANAGER_ID == managerselect@AGENT_ID,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> SOURCETODRCOUNT",
				"concatname select(mapColumn(",
				"          AGENT_ID = selectagent@AGENT_ID,",
				"          AGENT_FULLNAME,",
				"          UNIT_MANAGER_ID = selectagent@UNIT_MANAGER_ID,",
				"          MANAGER_FULLNAME,",
				"          DR_COUNT",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> FINALSELECT",
				"Bagentsource2 select(mapColumn(",
				"          AGENT_ID,",
				"          AGENT_FIRSTNAME,",
				"          AGENT_LASTNAME,",
				"          UNIT_MANAGER_ID",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectagent",
				"DRCOUNTGROUPBY select(mapColumn(",
				"          UNIT_MANAGER_ID,",
				"          AGENT_ID = AGENT_ID_B,",
				"          MANAGER_FULLNAME = AGENT_FULLNAME,",
				"          DR_COUNT",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> managerselect",
				"SOURCETODRCOUNT derive(AGENT_FULLNAME = concat(AGENT_FIRSTNAME,' ',AGENT_LASTNAME)) ~> concatname",
				"FINALSELECT sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          AGENT_ID,",
				"          AGENT_FULLNAME,",
				"          UNIT_MANAGER_ID,",
				"          MANAGER_FULLNAME,",
				"          DR_COUNT",
				"     )) ~> sink1"
			]
		}
	}
}