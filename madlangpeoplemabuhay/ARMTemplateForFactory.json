									"type": "AzureBlobFSWriteSettings"
								},
								"formatSettings": {
									"type": "ParquetWriteSettings"
								}
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"name": "AGENT_ID",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "AGENTID",
											"physicalType": "UTF8"
										}
									},
									{
										"source": {
											"name": "AGENT_FIRSTNAME",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "AGENTFIRSTNAME",
											"physicalType": "UTF8"
										}
									},
									{
										"source": {
											"name": "AGENT_LASTNAME",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "AGENTLASTNAME",
											"physicalType": "UTF8"
										}
									},
									{
										"source": {
											"name": "UNIT_MANAGER_ID",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "UNITMANAGER_ID",
											"physicalType": "UTF8"
										}
									}
								],
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "agent_source_dataset",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "agent_source_parquet_dataset",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/agent_source_dataset')]",
				"[concat(variables('factoryId'), '/datasets/agent_source_parquet_dataset')]"
			]
